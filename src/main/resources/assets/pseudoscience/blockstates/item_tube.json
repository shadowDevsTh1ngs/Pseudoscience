{
  "multipart": [
    {
      "apply": {
        "model": "pseudoscience:block/item_tube_center"
      },
      "when": {
        "straight": false
      }
    },
    {
      "apply": {
        "model": "pseudoscience:block/item_tube_connection_north"
      },
      "when": {
        "north": "connection|conduit",
        "straight": false
      }
    },
    {
      "apply": {
        "model": "pseudoscience:block/item_tube_connection_west"
      },
      "when": {
        "west": "connection|conduit",
        "straight": false
      }
    },
    {
      "apply": {
        "model": "pseudoscience:block/item_tube_connection_up"
      },
      "when": {
        "up": "connection|conduit",
        "straight": false
      }
    },
    {
      "apply": {
        "model": "pseudoscience:block/item_tube_connection_south"
      },
      "when": {
        "south": "connection|conduit",
        "straight": false
      }
    },
    {
      "apply": {
        "model": "pseudoscience:block/item_tube_connection_east"
      },
      "when": {
        "east": "connection|conduit",
        "straight": false
      }
    },
    {
      "apply": {
        "model": "pseudoscience:block/item_tube_connection_down"
      },
      "when": {
        "down": "connection|conduit",
        "straight": false
      }
    },
    {
      "apply": {
        "model": "pseudoscience:block/item_tube_straight"
      },
      "when": {
        "straight": true,
        "north": "connection|conduit"
      }
    },
    {
      "apply": {
        "model": "pseudoscience:block/item_tube_straight",
        "y": 90
      },
      "when": {
        "straight": true,
        "east": "connection|conduit"
      }
    },
    {
      "apply": {
        "model": "pseudoscience:block/item_tube_straight",
        "x": 90
      },
      "when": {
        "straight": true,
        "up": "connection|conduit"
      }
    }
  ]
}
